{
  "name": "smartcat/smartcat-api",
  "description": "PHP client for SmartCAT API https://smartcat.ai/api/methods/",
  "version": "2.1.3",
  "type": "library",
  "keywords": [
    "php",
    "smartcat",
    "api"
  ],
  "homepage": "https://github.com/smartcatai/SmartCAT-API",
  "license": "GPL-3.0-or-later",
  "support": {
    "issues": "https://github.com/smartcatai/SmartCAT-API/issues"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/panjiwiramanik/SmartCAT-API"
    }
  ],
  "require": {
    "php": "^7.1",
    "php-http/socket-client": "~2.0",
    "php-http/client-common": "~2.3",
    "php-http/message": "~1.0",
    "guzzlehttp/psr7": "~1.2",
    "php-http/multipart-stream-builder": "~1.0",
    "symfony/serializer": "^4.0 || ^5.0",
    "nesbot/carbon": "^2.42"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Andrey Rybnikov",
      "email": "a.rybnikov@smartcat.pro"
    }
  ],
  "autoload": {
    "psr-4": {
      "SmartCat\\Client\\": "src/SmartCAT/API"
    }
  }
}
